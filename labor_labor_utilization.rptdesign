            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

// Add query to sqlText variable.
sqlText = "SELECT person.locationsite, labor.laborcode, laborcraftrate.craft, personcal.shiftnum,  "
+ " labor.orgid, person.displayname, person.personid, "
+ " isnull( "
+ " (select sum(regularhrs) from labtrans a where labor.laborcode=a.laborcode and labor.orgid=a.orgid  "
+ " and a.startdate >= dateadd(wk, datediff(wk, +8, getdate()), -2)  "
+ " AND a.startdate < dateadd(wk, datediff(wk, +1, getdate()), -2)  ),0 ) as work_hours, "
+ " isnull((select sum(workhours) as avail_hours "
+ " from workperiod "
+ " where workdate >= dateadd(wk, datediff(wk, +8, getdate()), -2) "
+ " and workdate < dateadd(wk, datediff(wk, +1, getdate()), -2) and shiftnum=personcal.shiftnum),0) as avail_hours, "
+ " isnull((select sum( "
+ " (case when reasoncode='WORK'   "
+ " 		then workhours   "
+ "  		else -workhours  "
+ " 		end)) as work_hours "
+ " from modavail where personid=person.personid and  workdate >= dateadd(wk, datediff(wk, +8, getdate()), -2) "
+ " and workdate < dateadd(wk, datediff(wk, +1, getdate()), -2)), 0) as mod_hours "
+ " FROM  labor  "
+ " JOIN person ON person.personid=labor.personid  "
+ " join personcal on person.personid=personcal.personid and personcal.isprimary=1 "
+ " LEFT OUTER JOIN laborcraftrate ON (labor.laborcode=laborcraftrate.laborcode) AND (labor.orgid=laborcraftrate.orgid) "
// where clause
//+ " where " + params["where"]
+ " where laborcraftrate.defaultcraft = 1  "
+ "  and (labor.laborcode <> 'TESTUSER2' and labor.laborcode <> 'TESTUSER1' ) "
//+ " and (labor.labocode <> 'TestUser2' or labor.labocode <> 'testuser1' or labor.laborcode <> 'verlyj01') "
+ " AND laborcraftrate.craft in ('AUTOTECH','ELEC','GROUNDS','MECH', 'PDMTECH', 'UTIL', 'WELDER', 'FACMAINT') "
+ " AND labor.status='ACTIVE' and labor.worksite = ? "
//+ " AND labor.status='ACTIVE' and labor.worksite = 'JCC' "
+ " AND  LABOR.LABORCODE NOT IN ('AUTO','GRNDS01','MECH','MECHANIC') "
//grouping &amp; ordering
//+ " ORDER BY laborcraftrate.craft, labor.laborcode 

;

maximoDataSet.setQuery(sqlText);

maximoDataSet.setQueryParameterValue(1, params["worksite"].replace("=" , "").toUpperCase());
]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.
row["locationsite"] = maximoDataSet.getString("locationsite");
row["laborcode"] = maximoDataSet.getString("laborcode");
row["craft"] = maximoDataSet.getString("craft");
row["shiftnum"] = maximoDataSet.getString("shiftnum");
row["orgid"] = maximoDataSet.getString("orgid");
row["displayname"] = maximoDataSet.getString("displayname");
row["personid"] = maximoDataSet.getString("personid");

row["work_hours"] = maximoDataSet.getDouble("work_hours");
row["avail_hours"] = maximoDataSet.getDouble("avail_hours");
row["mod_hours"] = maximoDataSet.getDouble("mod_hours");

//row["datefield"] = maximoDataSet.getTimestamp("datefield");	

return (true);]]></method>
